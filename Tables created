

select * from HFS_RISKC_API_LOGS_MAINTAIN;

CREATE TABLE HFS_RISKC_API_LOGS_MAINTAIN (
    LOG_ID NUMBER GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
    RELATION_ID NVARCHAR2(255) NOT NULL,
    API_ENDPOINT NVARCHAR2(2000) NOT NULL,
    HTTP_METHOD NVARCHAR2(255) NOT NULL,
    REQUEST_PAYLOAD NCLOB,
    RESPONSE_PAYLOAD NCLOB,
    STATUS NVARCHAR2(255) NOT NULL,
    STATUS_CODE NUMBER(5) NOT NULL,
    EXCEPTION_DETAILS NCLOB,
    USER_ID NVARCHAR2(255),
    LOGGED_FROM NVARCHAR2(255) NOT NULL,
    CREATED_AT TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL
);




