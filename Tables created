select * from HFS_RISKC_CLIENT_CREDENTIALS

CREATE TABLE HFS_RISKC_CLIENT_CREDENTIALS (
    ENV NVARCHAR2(100) NOT NULL,
    PARTNER_CODE NVARCHAR2(100) NOT NULL,
    PARTNER_KEY NVARCHAR2(255) NOT NULL,
    PRIMARY KEY (ENV)
);


